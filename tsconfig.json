{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "types": ["bun-types"],
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@zemble/react-urql/*": ["packages/react-urql/*"],
      "@zemble/react-urql": ["packages/react-urql"],
      "@zemble/auth/*": ["packages/auth/*"],
      "@zemble/auth": ["packages/auth"],
      "@zemble/kv/*": ["packages/kv/*"],
      "@zemble/kv": ["packages/kv/plugin"],
      "@zemble/react-auth/*": ["packages/react-auth/*"],
      "@zemble/react-auth": ["packages/react-auth"],
      "@zemble/utils/*": ["packages/utils/*"],
      "@zemble/utils": ["packages/utils"],
      "@zemble/routes/*": ["packages/routes/*"],
      "@zemble/routes": ["packages/routes"],
      "@zemble/primitives/*": ["packages/primitives/*"],
      "@zemble/primitives": ["packages/primitives"],
      "@zemble/bull/*": ["packages/bull/*"],
      "@zemble/bull": ["packages/bull"],
      "@zemble/mongodb/*": ["packages/mongodb/*"],
      "@zemble/mongodb": ["packages/mongodb"],
      "@zemble/auth-otp/*": ["packages/auth-otp/*"],
      "@zemble/auth-otp": ["packages/auth-otp/plugin"],
      "@zemble/graphql/*": ["packages/graphql/*"],
      "@zemble/graphql": ["packages/graphql"],
      "@zemble/core/*": ["packages/core/*"],
      "@zemble/core": ["packages/core"],
      "@zemble/react/*": ["packages/react/*"],
      "@zemble/react": ["packages/react"],
      "@zemble/react-snackbar/*": ["packages/react-snackbar/*"],
      "@zemble/react-snackbar": ["packages/react-snackbar"],
      "@zemble/bun": ["packages/bun"],
      "@zemble/bun/*": ["packages/bun/*"],
      "@zemble/migrations/*": ["packages/migrations/*"],
      "@zemble/migrations": ["packages/migrations"],
      "@zemble/react-network/*": ["packages/react-network/*"],
      "@zemble/react-network": ["packages/react-network"],
      "@zemble/react-portal/*": ["packages/react-portal/*"],
      "@zemble/react-portal": ["packages/react-portal"],
      "@zemble/react-keyboard/*": ["packages/react-keyboard/*"],
      "@zemble/react-keyboard": ["packages/react-keyboard"]
    }
  },
  "exclude": [
    "**/*.generated/*.ts",
    "**/*.generated.ts",
    "**/*.test.ts",
    "**/*.d.ts",
    "**/*.d.mts",
    "**/*.mts",
    "node_modules",
    "dist"
  ]
}
