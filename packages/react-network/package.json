{
  "name": "@zemble/react-network",
  "version": "1.0.1",
  "description": "Network utilities for React Native",
  "author": "Robert Herber",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "(rm index.d.ts || true) && zshy"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "peerDependencies": {
    "@types/react": "^18.3.23",
    "react": "^18.3.1",
    "react-native": "~0.80.1"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-community/netinfo": "^9.5.0",
    "@zemble/react": "workspace:*",
    "@zemble/primitives": "workspace:*",
    "ts-pattern": "^5.7.1",
    "urql": "4.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "@types/react-native": "~0.73.0"
  },
  "zshy": {
    "exports": {
      ".": "./index.ts",
      "./hooks/*": "./hooks/**/*"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./hooks/*": {
      "types": "./hooks/*",
      "import": "./hooks/*",
      "require": "./hooks/*"
    }
  },
  "module": "./index.mjs"
}
