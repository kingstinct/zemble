{
  "name": "@zemble/push-expo",
  "version": "0.0.19",
  "description": "",
  "keywords": [
    "zemble",
    "zemble-plugin",
    "@zemble"
  ],
  "dependencies": {
    "@zemble/bun": "workspace:*",
    "@zemble/core": "workspace:*",
    "@zemble/graphql": "workspace:*",
    "expo-server-sdk": "^3.15.0",
    "@zemble/utils": "workspace:*"
  },
  "scripts": {
    "dev": "zemble-dev plugin.ts",
    "typecheck": "tsc --noEmit",
    "codegen": "graphql-codegen",
    "build": "zshy"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@tsconfig/bun": "^1.0.8"
  },
  "peerDependencies": {},
  "module": "plugin.ts",
  "main": "plugin.ts",
  "zshy": {
    "exports": {
      "./codegen": "./codegen.ts",
      "./getReceipts": "./getReceipts.ts",
      "./plugin": "./plugin.ts",
      "./graphql/*": "./graphql/**/*"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    "./codegen": {
      "types": "./codegen.d.ts",
      "import": "./codegen.mjs",
      "require": "./codegen.js"
    },
    "./getReceipts": {
      "types": "./getReceipts.d.ts",
      "import": "./getReceipts.mjs",
      "require": "./getReceipts.js"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    "./graphql/*": {
      "types": "./graphql/*",
      "import": "./graphql/*",
      "require": "./graphql/*"
    }
  }
}
