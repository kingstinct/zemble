{
  "name": "@zemble/primitives",
  "version": "1.0.1",
  "description": "Zemble React Native Primitives",
  "author": "Robert Herber",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "zshy"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "@types/react": "^18.3.23",
    "react": "^18.3.1",
    "react-native": "~0.80.1",
    "react-native-safe-area-context": "^5.5.1"
  },
  "dependencies": {
    "expo-system-ui": "^2.9.4",
    "ts-pattern": "^5.7.1"
  },
  "zshy": {
    "exports": {
      "./index": "./index.ts",
      "./src/*": "./src/*.ts"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ]
}
