{
  "name": "@zemble/react-urql",
  "version": "1.0.1",
  "description": "URQL helpers for Zemble React apps",
  "author": "Robert Herber",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "(rm index.d.ts || true) && zshy"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "peerDependencies": {
    "@types/react": "^18.3.23",
    "react": "^18.3.1",
    "react-native": "~0.80.1"
  },
  "dependencies": {
    "@zemble/react-auth": "workspace:*",
    "@zemble/utils": "workspace:*",
    "graphql": "^16.11.0",
    "urql": "4.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "@types/react-native": "~0.73.0"
  },
  "zshy": {
    "exports": {
      ".": "./index.ts"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "module": "./index.mjs"
}
