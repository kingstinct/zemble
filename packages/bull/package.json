{
  "name": "@zemble/bull",
  "version": "0.2.13",
  "description": "@zemble Plugin providing Bull Queue Support",
  "main": "plugin.ts",
  "scripts": {
    "dev": "docker-compose up -d && REDIS_URL=redis://localhost:6379 zemble-dev plugin.ts",
    "codegen": "graphql-codegen --config codegen.ts",
    "typecheck": "tsc --noEmit",
    "build": "(rm index.d.ts || true) && zshy"
  },
  "keywords": [
    "zemble",
    "zemble-plugin",
    "@zemble"
  ],
  "author": {
    "name": "Robert Herber",
    "email": "robert@kingstinct.com",
    "url": "https://github.com/robertherber"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kingstinct/zemble",
    "directory": "packages/apple-app-site-association"
  },
  "funding": [
    "https://github.com/sponsors/kingstinct",
    "https://github.com/sponsors/robertherber"
  ],
  "bugs": {
    "url": "https://github.com/kingstinct/zemble/issues"
  },
  "license": "ISC",
  "dependencies": {
    "@bull-board/api": "^6.11.0",
    "@types/ejs": "^3.1.5",
    "@zemble/auth": "workspace:*",
    "@zemble/core": "workspace:*",
    "@zemble/graphql": "workspace:*",
    "@zemble/utils": "workspace:*",
    "bullmq": "^5.56.1",
    "ejs": "^3.1.10",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "hono": "^4.8.4",
    "ioredis": "^5.6.1",
    "safe-stable-stringify": "^2.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@types/bun": "*"
  },
  "zshy": {
    "exports": {
      "./ZembleQueue": "./ZembleQueue.ts",
      "./ZembleQueueBull": "./ZembleQueueBull.ts",
      "./ZembleQueueMock": "./ZembleQueueMock.ts",
      "./bullboard-hono-adapter": "./bullboard-hono-adapter.ts",
      "./codegen": "./codegen.ts",
      "./plugin": "./plugin.ts",
      "./clients/*": "./clients/**/*",
      "./graphql/*": "./graphql/**/*",
      "./queues/*": "./queues/**/*",
      "./utils/*": "./utils/**/*"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    "./ZembleQueue": {
      "types": "./ZembleQueue.d.ts",
      "import": "./ZembleQueue.mjs",
      "require": "./ZembleQueue.js"
    },
    "./ZembleQueueBull": {
      "types": "./ZembleQueueBull.d.ts",
      "import": "./ZembleQueueBull.mjs",
      "require": "./ZembleQueueBull.js"
    },
    "./ZembleQueueMock": {
      "types": "./ZembleQueueMock.d.ts",
      "import": "./ZembleQueueMock.mjs",
      "require": "./ZembleQueueMock.js"
    },
    "./bullboard-hono-adapter": {
      "types": "./bullboard-hono-adapter.d.ts",
      "import": "./bullboard-hono-adapter.mjs",
      "require": "./bullboard-hono-adapter.js"
    },
    "./codegen": {
      "types": "./codegen.d.ts",
      "import": "./codegen.mjs",
      "require": "./codegen.js"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    "./clients/*": {
      "types": "./clients/*",
      "import": "./clients/*",
      "require": "./clients/*"
    },
    "./graphql/*": {
      "types": "./graphql/*",
      "import": "./graphql/*",
      "require": "./graphql/*"
    },
    "./queues/*": {
      "types": "./queues/*",
      "import": "./queues/*",
      "require": "./queues/*"
    },
    "./utils/*": {
      "types": "./utils/*",
      "import": "./utils/*",
      "require": "./utils/*"
    }
  }
}
