{
  "name": "@zemble/mongodb",
  "version": "0.0.44",
  "description": "Handles connections to MongoDB",
  "keywords": [
    "zemble",
    "zemble-plugin",
    "@zemble"
  ],
  "dependencies": {
    "@zemble/auth": "workspace:*",
    "@zemble/bun": "workspace:*",
    "@zemble/core": "workspace:*",
    "@zemble/routes": "workspace:*",
    "@zemble/utils": "workspace:*",
    "mongodb": "^6.17.0",
    "mongodb-memory-server": "10",
    "mongodb-memory-server-core": "^10.1.4",
    "type-fest": "^4.41.0"
  },
  "scripts": {
    "test": "bun test",
    "dev": "zemble-dev plugin.ts",
    "typecheck": "tsc --noEmit",
    "build": "(rm index.d.ts || true) && (rm plugin.d.ts || true) && zshy --project ./tsconfig.build.json"
  },
  "devDependencies": {
    "@zemble/migrations": "workspace:*"
  },
  "peerDependencies": {
    "@zemble/migrations": "0.0.40"
  },
  "module": "./plugin.mjs",
  "main": "./plugin.js",
  "zshy": {
    "exports": {
      "./migration-adapter-with-transactions": "./migration-adapter-with-transactions.ts",
      "./migration-adapter": "./migration-adapter.ts",
      "./migration.test": "./migration.test.ts",
      "./plugin": "./plugin.ts",
      ".": "./plugin.ts",
      "./test-setup": "./test-setup.ts",
      "./test-utils": "./test-utils.ts",
      "./routes/*": "./routes/**/*"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    "./migration-adapter-with-transactions": {
      "types": "./migration-adapter-with-transactions.d.ts",
      "import": "./migration-adapter-with-transactions.mjs",
      "require": "./migration-adapter-with-transactions.js"
    },
    "./migration-adapter": {
      "types": "./migration-adapter.d.ts",
      "import": "./migration-adapter.mjs",
      "require": "./migration-adapter.js"
    },
    "./migration.test": {
      "types": "./migration.test.d.ts",
      "import": "./migration.test.mjs",
      "require": "./migration.test.js"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    ".": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    "./test-setup": {
      "types": "./test-setup.d.ts",
      "import": "./test-setup.mjs",
      "require": "./test-setup.js"
    },
    "./test-utils": {
      "types": "./test-utils.d.ts",
      "import": "./test-utils.mjs",
      "require": "./test-utils.js"
    },
    "./routes/*": {
      "types": "./routes/*",
      "import": "./routes/*",
      "require": "./routes/*"
    }
  },
  "types": "./plugin.d.ts"
}
