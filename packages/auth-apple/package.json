{
  "name": "@zemble/auth-apple",
  "version": "0.0.71",
  "description": "Zemble Plugin providing authentication through Sign in with Apple",
  "main": "./plugin.js",
  "scripts": {
    "test": "bun test",
    "dev": "zemble-dev plugin.ts",
    "typecheck": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.ts",
    "build": "(rm index.d.ts || true) && (rm plugin.d.ts || true) && zshy --project ./tsconfig.build.json"
  },
  "keywords": [
    "zemble",
    "zemble-plugin",
    "@zemble"
  ],
  "author": {
    "name": "Robert Herber",
    "email": "robert@kingstinct.com",
    "url": "https://github.com/robertherber"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kingstinct/zemble",
    "directory": "packages/apple-app-site-association"
  },
  "funding": [
    "https://github.com/sponsors/kingstinct",
    "https://github.com/sponsors/robertherber"
  ],
  "bugs": {
    "url": "https://github.com/kingstinct/zemble/issues"
  },
  "license": "ISC",
  "dependencies": {
    "@envelop/core": "^5.3.0",
    "@envelop/generic-auth": "7",
    "@whatwg-node/server-plugin-cookies": "^1.0.5",
    "@zemble/core": "workspace:*",
    "@zemble/graphql": "workspace:*",
    "graphql": "^16.11.0",
    "jose": "^6.0.11",
    "@zemble/auth": "workspace:*"
  },
  "zshy": {
    "exports": {
      "./codegen": "./codegen.ts",
      "./plugin": "./plugin.ts",
      ".": "./plugin.ts",
      "./graphql/*": "./graphql/**/*",
      "./utils/*": "./utils/**/*"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    "./codegen": {
      "types": "./codegen.d.ts",
      "import": "./codegen.mjs",
      "require": "./codegen.js"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    ".": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    "./graphql/*": {
      "types": "./graphql/*",
      "import": "./graphql/*",
      "require": "./graphql/*"
    },
    "./utils/*": {
      "types": "./utils/*",
      "import": "./utils/*",
      "require": "./utils/*"
    }
  },
  "module": "./plugin.mjs",
  "types": "./plugin.d.ts"
}
