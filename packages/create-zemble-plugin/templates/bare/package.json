{
  "name": "pkgname",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "zemble",
    "zemble-plugin",
    "@zemble"
  ],
  "dependencies": {
    "@zemble/core": "workspace:*",
    "@zemble/bun": "workspace:*"
  },
  "scripts": {
    "test": "bun test",
    "dev": "zemble-dev plugin.ts",
    "typecheck": "tsc --noEmit",
    "build": "(rm index.d.ts || true) && (rm plugin.d.ts || true) && zshy --project ./tsconfig.build.json"
  },
  "devDependencies": {
    "@tsconfig/bun": "^1.0.1"
  },
  "peerDependencies": {},
  "module": "plugin.ts",
  "main": "plugin.ts",
  "zshy": {
    "exports": {
      "./plugin.test-template": "./plugin.test-template.ts",
      "./plugin": "./plugin.ts"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ]
}
