{
  "name": "@zemble/firebase-auth",
  "version": "0.0.24",
  "description": "",
  "keywords": [
    "zemble",
    "zemble-plugin",
    "@zemble"
  ],
  "dependencies": {
    "@zemble/bun": "workspace:*",
    "@zemble/core": "workspace:*",
    "@zemble/graphql": "workspace:*",
    "@zemble/routes": "workspace:*",
    "@zemble/auth": "workspace:*",
    "@zemble/utils": "workspace:*",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0"
  },
  "scripts": {
    "dev": "zemble-dev plugin.ts",
    "typecheck": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.ts",
    "build": "(rm index.d.ts || true) && zshy"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@tsconfig/bun": "^1.0.8"
  },
  "peerDependencies": {},
  "module": "plugin.ts",
  "main": "plugin.ts",
  "zshy": {
    "exports": {
      "./codegen": "./codegen.ts",
      "./plugin": "./plugin.ts",
      "./clients/*": "./clients/**/*",
      "./graphql/*": "./graphql/**/*",
      "./utils/*": "./utils/**/*"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    "./codegen": {
      "types": "./codegen.d.ts",
      "import": "./codegen.mjs",
      "require": "./codegen.js"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    "./clients/*": {
      "types": "./clients/*",
      "import": "./clients/*",
      "require": "./clients/*"
    },
    "./graphql/*": {
      "types": "./graphql/*",
      "import": "./graphql/*",
      "require": "./graphql/*"
    },
    "./utils/*": {
      "types": "./utils/*",
      "import": "./utils/*",
      "require": "./utils/*"
    }
  }
}
