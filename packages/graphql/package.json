{
  "name": "@zemble/graphql",
  "version": "0.0.63",
  "description": "Zemble Plugin providing GraphQL support for your app",
  "main": "plugin.ts",
  "bin": {
    "zemble-app-graphql-codegen": "./bin/zemble-app-graphql-codegen.ts"
  },
  "type": "module",
  "scripts": {
    "dev": "zemble-dev plugin.ts",
    "codegen": "graphql-codegen",
    "typecheck": "tsc --noEmit",
    "build": "(zshy || true) && node -e \"const fs=require('fs');const pkg=JSON.parse(fs.readFileSync('./package.json'));if(pkg.exports){Object.keys(pkg.exports).forEach(key=>{['require','import','types'].forEach(type=>{if(pkg.exports[key][type]&&!fs.existsSync(pkg.exports[key][type].replace('./',''))){delete pkg.exports[key][type]}})});fs.writeFileSync('./package.json',JSON.stringify(pkg,null,2)+String.fromCharCode(10))}\""
  },
  "keywords": [
    "zemble",
    "zemble-plugin",
    "@zemble"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kingstinct/zemble",
    "directory": "packages/graphql"
  },
  "funding": [
    "https://github.com/sponsors/kingstinct",
    "https://github.com/sponsors/robertherber"
  ],
  "author": {
    "name": "Robert Herber",
    "email": "robert@kingstinct.com",
    "url": "https://github.com/robertherber"
  },
  "bugs": {
    "url": "https://github.com/kingstinct/zemble/issues"
  },
  "license": "ISC",
  "dependencies": {
    "@eddeee888/gcg-typescript-resolver-files": "^0.12.1",
    "@envelop/core": "^5.3.0",
    "@envelop/on-resolve": "^4.1.1",
    "@graphql-codegen/plugin-helpers": "^5.1.1",
    "@graphql-tools/delegate": "^10.2.20",
    "@graphql-tools/graphql-file-loader": "^8.0.20",
    "@graphql-tools/load": "^8.1.0",
    "@graphql-tools/merge": "^9.0.24",
    "@graphql-tools/schema": "^10.0.23",
    "@graphql-tools/utils": "^10.8.6",
    "@graphql-tools/wrap": "^10.1.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@zemble/core": "workspace:*",
    "debug": "^4.4.1",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "graphql-ws": "^5.16.2",
    "graphql-yoga": "^5.14.0",
    "hono": "^4.8.4",
    "jose": "^6.0.11",
    "sofa-api": "^0.18.8"
  },
  "optionalDependencies": {
    "ioredis": "^5.4.2",
    "@graphql-yoga/redis-event-target": "^3.0.3",
    "sofa-api": "^0.18.8"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "ioredis": "^5.6.1",
    "@graphql-yoga/redis-event-target": "^3.0.3"
  },
  "zshy": {
    "exports": {
      "./codegen": "./codegen.ts",
      "./createPubSub": "./createPubSub.ts",
      "./middleware": "./middleware.ts",
      "./plugin": "./plugin.ts",
      "./clients/*": "./clients/**/*",
      "./graphql/*": "./graphql/**/*",
      "./utils/*": "./utils/**/*"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    "./codegen": {
      "types": "./codegen.d.cts",
      "import": "./codegen.js",
      "require": "./codegen.cjs"
    },
    "./createPubSub": {
      "types": "./createPubSub.d.cts",
      "import": "./createPubSub.js",
      "require": "./createPubSub.cjs"
    },
    "./middleware": {
      "types": "./middleware.d.cts",
      "import": "./middleware.js",
      "require": "./middleware.cjs"
    },
    "./plugin": {
      "types": "./plugin.d.cts",
      "import": "./plugin.js",
      "require": "./plugin.cjs"
    },
    "./clients/*": {},
    "./graphql/*": {},
    "./utils/*": {}
  }
}
