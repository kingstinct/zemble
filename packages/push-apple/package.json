{
  "name": "@zemble/push-apple",
  "version": "0.0.21",
  "description": "",
  "keywords": [
    "zemble",
    "zemble-plugin",
    "@zemble"
  ],
  "dependencies": {
    "@zemble/bun": "workspace:*",
    "@zemble/core": "workspace:*",
    "@zemble/graphql": "workspace:*",
    "@zemble/routes": "workspace:*",
    "@zemble/utils": "workspace:*",
    "fetch-http2": "^1.5.0",
    "graphql-scalars": "^1.24.2",
    "jose": "^6.0.11"
  },
  "scripts": {
    "dev": "zemble-dev plugin.ts",
    "typecheck": "tsc --noEmit",
    "codegen": "graphql-codegen",
    "build": "zshy"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@tsconfig/bun": "^1.0.8"
  },
  "peerDependencies": {},
  "module": "plugin.ts",
  "main": "plugin.ts",
  "zshy": {
    "exports": {
      "./codegen": "./codegen.ts",
      "./plugin": "./plugin.ts",
      "./types": "./types.ts",
      "./utils": "./utils.ts",
      "./clients/*": "./clients/**/*",
      "./graphql/*": "./graphql/**/*"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    "./codegen": {
      "types": "./codegen.d.ts",
      "import": "./codegen.mjs",
      "require": "./codegen.js"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    "./types": {
      "types": "./types.d.ts",
      "import": "./types.mjs",
      "require": "./types.js"
    },
    "./utils": {
      "types": "./utils.d.ts",
      "import": "./utils.mjs",
      "require": "./utils.js"
    },
    "./clients/*": {
      "types": "./clients/*",
      "import": "./clients/*",
      "require": "./clients/*"
    },
    "./graphql/*": {
      "types": "./graphql/*",
      "import": "./graphql/*",
      "require": "./graphql/*"
    }
  }
}
