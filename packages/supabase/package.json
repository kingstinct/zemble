{
  "name": "@zemble/supabase",
  "version": "0.0.48",
  "description": "",
  "keywords": [
    "zemble",
    "zemble-plugin",
    "@zemble"
  ],
  "dependencies": {
    "@zemble/core": "workspace:*",
    "@zemble/bun": "workspace:*",
    "@zemble/auth": "workspace:*",
    "@zemble/routes": "workspace:*",
    "@zemble/graphql": "workspace:*",
    "@supabase/ssr": "^0.6.1",
    "graphql": "^16.11.0",
    "@supabase/supabase-js": "^2.50.3"
  },
  "scripts": {
    "dev": "zemble-dev plugin.ts",
    "typecheck": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.ts",
    "supabase-codegen": "supabase gen types typescript --project-id \"nptqmmaxmynahsgfuvhn\" --schema public > types/supabase.generated.ts",
    "build": "zshy --project ./tsconfig.build.json"
  },
  "devDependencies": {
    "supabase": "^2.31.0",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@tsconfig/bun": "^1.0.8"
  },
  "peerDependencies": {},
  "module": "./plugin.mjs",
  "main": "./plugin.js",
  "zshy": {
    "exports": {
      "./codegen": "./codegen.ts",
      "./decodeToken": "./decodeToken.ts",
      "./plugin": "./plugin.ts",
      ".": "./plugin.ts",
      "./test-utils": "./test-utils.ts",
      "./clients/*": "./clients/**/*",
      "./graphql/*": "./graphql/**/*",
      "./routes/*": "./routes/**/*"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    "./codegen": {
      "types": "./codegen.d.ts",
      "import": "./codegen.mjs",
      "require": "./codegen.js"
    },
    "./decodeToken": {
      "types": "./decodeToken.d.ts",
      "import": "./decodeToken.mjs",
      "require": "./decodeToken.js"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    ".": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    "./test-utils": {
      "types": "./test-utils.d.ts",
      "import": "./test-utils.mjs",
      "require": "./test-utils.js"
    },
    "./clients/*": {
      "types": "./clients/*",
      "import": "./clients/*",
      "require": "./clients/*"
    },
    "./graphql/*": {
      "types": "./graphql/*",
      "import": "./graphql/*",
      "require": "./graphql/*"
    },
    "./routes/*": {
      "types": "./routes/*",
      "import": "./routes/*",
      "require": "./routes/*"
    }
  },
  "types": "./plugin.d.ts"
}
