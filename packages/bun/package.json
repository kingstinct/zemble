{
  "name": "@zemble/bun",
  "version": "0.0.38",
  "description": "@zemble Plugin for running bun",
  "main": "serve.ts",
  "bin": {
    "zemble-dev": "./cli.ts"
  },
  "scripts": {
    "dev": "bunx cli.ts",
    "typecheck": "tsc --noEmit",
    "build": "zshy"
  },
  "keywords": [
    "zemble",
    "@zemble"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kingstinct/zemble",
    "directory": "packages/bun"
  },
  "funding": [
    "https://github.com/sponsors/kingstinct",
    "https://github.com/sponsors/robertherber"
  ],
  "author": {
    "name": "Robert Herber",
    "email": "robert@kingstinct.com",
    "url": "https://github.com/robertherber"
  },
  "bugs": {
    "url": "https://github.com/kingstinct/zemble/issues"
  },
  "license": "ISC",
  "dependencies": {
    "@zemble/core": "workspace:*"
  },
  "zshy": {
    "exports": {
      "./cli": "./cli.ts",
      "./serve": "./serve.ts"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    "./cli": {
      "types": "./cli.d.ts",
      "import": "./cli.mjs",
      "require": "./cli.js"
    },
    "./serve": {
      "types": "./serve.d.ts",
      "import": "./serve.mjs",
      "require": "./serve.js"
    }
  }
}
