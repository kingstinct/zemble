{
  "name": "@zemble/routes",
  "version": "0.0.39",
  "description": "@zemble Plugin for routing and static files",
  "main": "plugin.ts",
  "scripts": {
    "dev": "zemble-dev plugin.ts",
    "typecheck": "tsc --noEmit",
    "build": "zshy"
  },
  "keywords": [
    "zemble",
    "zemble-plugin",
    "@zemble"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kingstinct/zemble",
    "directory": "packages/route"
  },
  "funding": [
    "https://github.com/sponsors/kingstinct",
    "https://github.com/sponsors/robertherber"
  ],
  "author": {
    "name": "Robert Herber",
    "email": "robert@kingstinct.com",
    "url": "https://github.com/robertherber"
  },
  "bugs": {
    "url": "https://github.com/kingstinct/zemble/issues"
  },
  "license": "ISC",
  "dependencies": {
    "@zemble/core": "workspace:*",
    "hono": "^4.8.4"
  },
  "zshy": {
    "exports": {
      "./middleware": "./middleware.ts",
      "./plugin": "./plugin.ts",
      "./utils/*": "./utils/**/*"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    "./middleware": {
      "types": "./middleware.d.ts",
      "import": "./middleware.mjs",
      "require": "./middleware.js"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    "./utils/*": {
      "types": "./utils/*",
      "import": "./utils/*",
      "require": "./utils/*"
    }
  }
}
