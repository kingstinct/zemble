{
  "name": "@zemble/auth-anonymous",
  "version": "0.0.65",
  "description": "Zemble Plugin providing Anonymous Auth support",
  "main": "./plugin.js",
  "scripts": {
    "dev": "zemble-dev plugin.ts",
    "typecheck": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.ts",
    "build": "(rm index.d.ts || true) && (rm plugin.d.ts || true) && zshy --project ./tsconfig.build.json"
  },
  "keywords": [
    "zemble",
    "zemble-plugin",
    "@zemble"
  ],
  "author": {
    "name": "Robert Herber",
    "email": "robert@kingstinct.com",
    "url": "https://github.com/robertherber"
  },
  "license": "ISC",
  "dependencies": {
    "@envelop/core": "^5.3.0",
    "@envelop/generic-auth": "7",
    "@whatwg-node/server-plugin-cookies": "^1.0.5",
    "@zemble/auth": "workspace:*",
    "@zemble/core": "workspace:*",
    "@zemble/graphql": "workspace:*",
    "@zemble/utils": "workspace:*",
    "graphql": "^16.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7"
  },
  "zshy": {
    "exports": {
      "./codegen": "./codegen.ts",
      "./plugin": "./plugin.ts",
      ".": "./plugin.ts",
      "./graphql/*": "./graphql/**/*"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    "./codegen": {
      "types": "./codegen.d.ts",
      "import": "./codegen.mjs",
      "require": "./codegen.js"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    ".": {
      "types": "./plugin.d.ts",
      "import": "./plugin.mjs",
      "require": "./plugin.js"
    },
    "./graphql/*": {
      "types": "./graphql/*",
      "import": "./graphql/*",
      "require": "./graphql/*"
    }
  },
  "module": "./plugin.mjs",
  "types": "./plugin.d.ts"
}
