{
  "name": "pkgname",
  "version": "0.0.1",
  "description": "",
  "dependencies": {
    "@zemble/core": "latest",
    "@zemble/bun": "latest",
    "@zemble/routes": "latest",
    "@zemble/graphql": "latest"
  },
  "scripts": {
    "start": "bun serve.ts",
    "test": "bun test",
    "dev": "bun --hot serve.ts",
    "typecheck": "tsc --noEmit",
    "codegen": "bunx zemble-app-graphql-codegen config.ts && graphql-codegen",
    "build": "zshy"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@tsconfig/bun": "^1.0.1"
  },
  "peerDependencies": {},
  "module": "serve.ts",
  "main": "serve.ts",
  "zshy": {
    "exports": {
      "./serve": "./serve.ts",
      "./graphql/*": "./graphql/*.ts",
      "./routes/*": "./routes/*.ts"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ]
}
