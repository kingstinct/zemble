{
  "name": "@zemble/node",
  "version": "0.0.38",
  "description": "@zemble Plugin for Node.js runtime",
  "main": "serve.js",
  "module": "serve.mjs",
  "types": "serve.d.ts",
  "bin": {
    "zemble-node": "cli.ts"
  },
  "scripts": {
    "start": "node cli.js",
    "dev": "ts-node-dev cli.ts",
    "typecheck": "tsc --noEmit",
    "build": "zshy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kingstinct/zemble",
    "directory": "packages/apple-app-site-association"
  },
  "funding": [
    "https://github.com/sponsors/kingstinct",
    "https://github.com/sponsors/robertherber"
  ],
  "bugs": {
    "url": "https://github.com/kingstinct/zemble/issues"
  },
  "keywords": [
    "zemble",
    "@zemble"
  ],
  "author": {
    "name": "Robert Herber",
    "email": "robert@kingstinct.com",
    "url": "https://github.com/robertherber"
  },
  "license": "ISC",
  "dependencies": {
    "@zemble/core": "workspace:*",
    "@hono/node-server": "1.15.0"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0"
  },
  "zshy": {
    "exports": {
      "./serve": "./serve.ts",
      "./cli": "./cli.ts"
    }
  },
  "exclude": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cts",
    "**/*.mts",
    "node_modules"
  ],
  "files": [
    "*"
  ],
  "exports": {
    "./serve": {
      "types": "./serve.d.ts",
      "import": "./serve.mjs",
      "require": "./serve.js"
    },
    "./cli": {
      "types": "./cli.d.ts",
      "import": "./cli.mjs",
      "require": "./cli.js"
    }
  }
}
